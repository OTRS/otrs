<!-- start customer table -->
<table border="0" cellspacing="1" cellpadding="0">
<!-- dtl:block:CustomerRow -->
  <tr>
    <td class="contentkey">$Text{"$QData{"Key"}"}:</td>
    <td class="contentvalue">$Data{"LinkStart"}<div title="$QData{"Value"}">$Data{"ValueShort"}</div>$Data{"LinkStop"}</td>
  </tr>
<!-- dtl:block:CustomerRow -->
</table>
<!-- stop customer table -->
